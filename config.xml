<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns     = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "ec.pocket.interlab"
        version   = "2.0.0">
    <name>interlab</name>
		<description>
		</description>
		<author href="http://keyframe.com/" email="epulgar@keyframe.ec">
Epulgar
